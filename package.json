{
  "name": "nuxt-shopify",
  "version": "1.8.0",
  "description": "Easy Shopify Buy client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "shopify",
    "shopify-buy",
    "nuxt-shopify"
  ],
  "typings": "types/index.d.ts",
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-shopify",
  "homepage": "https://github.com/Gomah/nuxt-shopify",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "prepublishOnly": "yarn build",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "build": "tsc -b",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js,.ts, src/"
  },
  "dependencies": {
    "@types/shopify-buy": "^1.11.0",
    "isomorphic-unfetch": "^3.0.0",
    "shopify-buy": "2.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nuxt/types": "^0.6.4",
    "@types/consola": "^2.2.5",
    "@types/jest": "^25.1.3",
    "@types/jsdom": "^16.1.0",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "nuxt": "^2.11.0",
    "prettier": "^1.19.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}

{
  "name": "nuxt-shopify",
  "version": "1.10.7",
  "description": "Easy Shopify Buy client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "shopify",
    "shopify-buy",
    "nuxt-shopify"
  ],
  "typings": "types/index.d.ts",
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-shopify",
  "homepage": "https://nuxt-shopify-docs.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "prepublishOnly": "yarn build",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "build": "tsc -b",
    "release": "yarn test && HUSKY_SKIP_HOOKS=1 standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test": "yarn test:lint && NODE_ENV=test jest",
    "test:lint": "eslint --ext .js,.ts, src/"
  },
  "dependencies": {
    "@types/shopify-buy": "^2.10.5",
    "isomorphic-unfetch": "^3.1.0",
    "shopify-buy": "2.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxt/types": "^2.15.3",
    "@types/consola": "^2.2.5",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nuxt": "^2.15.3",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}

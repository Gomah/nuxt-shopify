{
  "name": "nuxt-shopify",
  "version": "1.0.5",
  "description": "Easy Shopify Buy client integration with Nuxt.js",
  "author": {
    "name": "Gomah <hi@gomah.fr>"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "shopify",
    "shopify-buy",
    "nuxt-shopify"
  ],
  "main": "lib/module.js",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "repository": "https://github.com/Gomah/nuxt-shopify",
  "homepage": "https://github.com/Gomah/nuxt-shopify",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "NODE_ENV=test jest",
    "prepublishOnly": "yarn build",
    "test:system": "NODE_ENV=test jest --testRegex \"/test/system/(.+)\\.test\\.ts$\"",
    "build": "tsc",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "watch": "tsc --watch",
    "test:lint": "eslint --ext .js,.ts, src/"
  },
  "dependencies": {
    "consola": "^2.7.1",
    "isomorphic-fetch": "^2.2.1",
    "shopify-buy": "^2.2.4"
  },
  "devDependencies": {
    "@types/consola": "^1.0.0",
    "@types/jest": "^24.0.13",
    "@types/jsdom": "^12.2.3",
    "@types/node": "^12.0.2",
    "@types/shopify-buy": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nuxt": "^2.7.1",
    "prettier": "^1.17.1",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
